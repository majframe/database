{
    "name": "majframe/database",
    "description": "Doctrine ORM Database extension for Majframe.",
    "type": "library",
    "minimum-stability": "stable",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ond≈ôej Maxa",
            "homepage": "http://ondrej.maxa.expert",
            "email": "ondrej@maxa.expert"
        }
    ],
    "autoload": {
        "psr-4": {
            "Majframe\\Database\\": "src/"
        }
    },
    "require": {
        "php": "^7.4",
        "doctrine/orm": "^2.7.2",
        "doctrine/annotations": "^1.10.2",
        "php-di/php-di": "^6.1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "scripts": {
        "codecept": "codecept run",
        "codecept-steps": "codecept run --steps"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}